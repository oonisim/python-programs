   ],
   "source": [
    "from itertools import groupby\n",
    "prefix = ''\n",
    "for k, g in groupby(\"aaabb11ccccc\"):\n",
    "    print(f\"{prefix}({k}, {len(list(g))})\", end='')\n",
    "    prefix = ' '\n"
   ]
